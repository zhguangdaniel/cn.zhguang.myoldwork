<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>
	Network
	</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<error-page>
	<error-code>404</error-code>
	<location>/jsp/error/pageNotFound.html</location>
	</error-page>
	<session-config>
        <session-timeout>30</session-timeout>
    </session-config>
    
    <servlet>
    	<servlet-name>RegisterJSP</servlet-name>
    	<jsp-file>/jsp/user/register.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
    	<servlet-name>RegisterJSP</servlet-name>
    	<url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
    	<servlet-name>LogoutJSP</servlet-name>
    	<jsp-file>/jsp/user/logout.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
    	<servlet-name>LogoutJSP</servlet-name>
    	<url-pattern>/logout</url-pattern>
    </servlet-mapping>
        <servlet>
    	<servlet-name>MyIndexJSP</servlet-name>
    	<jsp-file>/jsp/myIndex/myIndex.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
    	<servlet-name>MyIndexJSP</servlet-name>
    	<url-pattern>/myIndex</url-pattern>
    </servlet-mapping>
</web-app>
